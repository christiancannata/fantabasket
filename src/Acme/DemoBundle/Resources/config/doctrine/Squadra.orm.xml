<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Acme\DemoBundle\Entity\Squadra" table="Squadra">
    <indexes>
      <index name="fk_Squadra_Utente1_idx" columns="id_utente"/>
    </indexes>
    <id name="id" type="integer" column="id"/>
    <id name="idUtente" association-key="true"/>
    <one-to-one field="idUtente" target-entity="Utente">
      <join-columns>
        <join-column name="id_utente" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-many field="giornata" target-entity="Giornata" mapped-by="squadra"/>
  </entity>
</doctrine-mapping>
